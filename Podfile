# Uncomment the next line to define a global platform for your project
# platform :ios, '10.0'
source 'https://github.com/CocoaPods/Specs.git'
target 'taipeipark' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for taipeipark
  pod 'Alamofire'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod ’SVProgressHUD’
end
target 'taipeiparkTests' do
use_frameworks!

  # Pods for taipeiparkTests
  pod 'Alamofire'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod ’SVProgressHUD’
end
